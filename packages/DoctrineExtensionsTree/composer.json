{
    "name": "symplify/doctrine-extensions-tree",
    "description": "Implementation of Tree from DoctrineExtensions to Nette.",
    "require": {
        "php": "^7.1",
        "gedmo/doctrine-extensions": "^2.4",
        "nette/di": "^2.4",
        "nette/reflection": "^2.4"
    },
    "require-dev": {
        "kdyby/doctrine": "^3.1",
        "nette/bootstrap": "^2.4",
        "tracy/tracy": "^2.4",
        "phpunit/phpunit": "^6.0",
        "symplify/coding-standard": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\DoctrineExtensionsTree\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\DoctrineExtensionsTree\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/phpcs src tests -ps --standard=vendor/symplify/coding-standard/src/SymplifyCodingStandard/ruleset.xml"
    }
}
