{
    "name": "symplify/easy-hydrator",
    "description": "Hydrate arrays to objects easily with PHP 7.4 and constructor injection",
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.0",
        "phpstan/phpdoc-parser": "^0.5",
        "symfony/cache": "^5.3",
        "symfony/config": "^5.3",
        "symfony/dependency-injection": "^5.3",
        "symfony/http-kernel": "^5.3",
        "symplify/simple-php-doc-parser": "^9.4.5",
        "symplify/autowire-array-parameter": "^9.4.5",
        "symplify/package-builder": "^9.4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\EasyHydrator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\EasyHydrator\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
