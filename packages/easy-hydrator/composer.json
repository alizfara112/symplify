{
    "name": "symplify/easy-hydrator",
    "description": "Hydrate arrays to objects easily with PHP 7.4 and constructor injection",
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.2",
        "symfony/cache": "^4.4|^5.0",
        "symfony/config": "^4.4|^5.0",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "symplify/autowire-array-parameter": "^7.2.12",
        "symplify/package-builder": "^7.2.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\EasyHydrator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\EasyHydrator\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.3-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
