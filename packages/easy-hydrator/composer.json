{
    "name": "symplify/easy-hydrator",
    "description": "Hydrate arrays to objects easily with PHP 7.4 and constructor injection",
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.3",
        "phpstan/phpdoc-parser": "^0.4.9",
        "symfony/cache": "^4.4|^5.2",
        "symfony/config": "^4.4|^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/http-kernel": "^4.4|^5.2",
        "symplify/simple-php-doc-parser": "^9.3",
        "symplify/autowire-array-parameter": "^9.3",
        "symplify/package-builder": "^9.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\EasyHydrator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\EasyHydrator\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.3-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
