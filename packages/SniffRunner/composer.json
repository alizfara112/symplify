{
    "name": "symplify/sniff-runner",
    "description": "SniffRunner finds all sniffs and runes them on your code.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-tokenizer": "*",
        "ext-simplexml": "*",
        "tomas-votruba/php_codesniffer-3-pretagged": "^3.0",
        "gherkins/regexpbuilderphp": "^0.7",
        "symfony/event-dispatcher": "^3.2",
        "symfony/finder": "^3.2",
        "nette/bootstrap": "^2.4",
        "nette/di": "^2.4",
        "nette/robot-loader": "^2.4",
        "nette/utils": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "tracy/tracy": "^2.4",
        "slevomat/coding-standard": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\SniffRunner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\SniffRunner\\Tests\\": "tests"
        }
    }
}
