{
    "name": "symplify/modular-routing",
    "description": "Load your Symfony routes with a simple service.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "symfony/config": "v3.3.0-RC1",
        "symfony/dependency-injection": "v3.3.0-RC1",
        "symfony/framework-bundle": "v3.3.0-RC1",
        "symfony/http-kernel": "v3.3.0-RC1",
        "symfony/asset": "v3.3.0-RC1",
        "symfony/routing": "v3.3.0-RC1",
        "symfony/templating": "v3.3.0-RC1",
        "symfony/validator": "v3.3.0-RC1",
        "symfony/twig-bundle": "v3.3.0-RC1",
        "symfony/yaml": "v3.3.0-RC1",
        "symfony-cmf/routing-bundle": "^2.0",
        "doctrine/doctrine-bundle": "^1.6"
    },
    "require-dev": {
        "nette/utils": "^2.4",
        "nette/finder": "^2.4",
        "phpunit/phpunit": "^6.0",
        "symfony/form": "v3.3.0-RC1",
        "twig/twig": "^2.2",
        "doctrine/annotations": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ModularRouting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ModularRouting\\Tests\\": "tests"
        }
    }
}
