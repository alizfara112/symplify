{
    "name": "symplify/modular-routing",
    "description": "Load your Symfony routes with a simple service.",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "symfony/config": "^3.0",
        "symfony/dependency-injection": "^3.0",
        "symfony/framework-bundle": "^3.0",
        "symfony/http-kernel": "^3.0",
        "symfony/asset": "^3.0",
        "symfony/routing": "^3.0",
        "symfony/templating": "^3.0",
        "symfony/twig-bundle": "^3.0",
        "symfony-cmf/routing-bundle": "^1.4"
    },
    "require-dev": {
        "nette/utils": "^2.4",
        "phpunit/phpunit": "^5.5",
        "symfony/form": "^3.0",
        "symplify/coding-standard": "^1.0",
        "twig/twig": "^1.24"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ModularRouting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ModularRouting\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/symplify-cs check src tests",
        "fix-cs": "vendor/bin/symplify-cs fix src tests"
    }
}
