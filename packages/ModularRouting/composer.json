{
    "name": "symplify/modular-routing",
    "description": "Load your Symfony routes with a simple service.",
    "require": {
        "php": "^7.1",
        "symfony/config": "^3.2",
        "symfony/dependency-injection": "^3.2",
        "symfony/framework-bundle": "^3.2",
        "symfony/http-kernel": "^3.2",
        "symfony/asset": "^3.2",
        "symfony/routing": "^3.2",
        "symfony/templating": "^3.2",
        "symfony/validator": "^3.2",
        "symfony/twig-bundle": "^3.2",
        "symfony/yaml": "^3.2",
        "symfony-cmf/routing-bundle": "^2.0",
        "doctrine/doctrine-bundle": "^1.6"
    },
    "require-dev": {
        "nette/utils": "^2.4",
        "nette/finder": "^2.4",
        "phpunit/phpunit": "^6.0",
        "symfony/form": "^3.2",
        "twig/twig": "^2.2",
        "doctrine/annotations": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ModularRouting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ModularRouting\\Tests\\": "tests"
        }
    }
}
