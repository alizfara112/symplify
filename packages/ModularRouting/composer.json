{
    "name": "symplify/modular-routing",
    "description": "Load your Symfony routes with a simple service.",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "symfony/config": "^3.1",
        "symfony/dependency-injection": "^3.1",
        "symfony/framework-bundle": "^3.1",
        "symfony/http-kernel": "^3.1",
        "symfony/asset": "^3.1",
        "symfony/routing": "^3.1",
        "symfony/templating": "^3.1",
        "symfony/validator": "^3.1",
        "symfony/twig-bundle": "^3.1",
        "symfony-cmf/routing-bundle": "dev-master as 1.4"
    },
    "require-dev": {
        "nette/utils": "^2.4",
        "nette/finder": "^2.4",
        "phpunit/phpunit": "^5.7",
        "symfony/form": "^3.1",
        "symplify/coding-standard": "~1.2",
        "twig/twig": "^1.28",
        "doctrine/annotations": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ModularRouting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ModularRouting\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/symplify-cs check src tests",
        "fix-cs": "vendor/bin/symplify-cs fix src tests"
    }
}
