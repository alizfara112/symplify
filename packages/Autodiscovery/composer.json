{
    "name": "symplify/autodiscovery",
    "description": "Forget manual registration of Doctrine entities, Twig templates and routes. Let autodiscovery handle that for you.",
    "license": "MIT",
    "bin": [
        "bin/autodiscovery"
    ],
    "require": {
        "php": "^7.2",
        "nette/utils": "^3.0",
        "symfony/console": "^3.4|^4.3|^5.0",
        "symfony/dependency-injection": "^3.4.10|^4.3|^5.0",
        "symfony/finder": "^3.4|^4.3|^5.0",
        "symfony/routing": "^3.4|^4.3|^5.0",
        "symplify/package-builder": "^6.2"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.11",
        "doctrine/orm": "^2.6.4",
        "symfony/framework-bundle": "^3.4|^4.3|^5.0",
        "symfony/http-kernel": "^3.4|^4.3|^5.0",
        "symfony/twig-bundle": "^3.4|^4.3|^5.0",
        "symfony/templating": "^3.4|^4.3|^5.0",
        "symfony/translation": "^3.4|^4.3|^5.0",
        "phpunit/phpunit": "^7.5|^8.4"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\Autodiscovery\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\Autodiscovery\\Tests\\": "tests"
        },
        "classmap": [
            "tests/Source/KernelProjectDir"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
