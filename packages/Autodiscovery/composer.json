{
    "name": "symplify/autodiscovery",
    "description": "Forget manual registration of Doctrine entities, Twig templates and routes. Let autodiscovery handle that for you.",
    "license": "MIT",
    "bin": [
        "bin/autodiscovery"
    ],
    "require": {
        "php": "^7.1",
        "nette/utils": "^2.5|^3.0",
        "symfony/console": "^3.4|^4.2",
        "symfony/dependency-injection": "^3.4.10|^4.2",
        "symfony/finder": "^3.4|^4.2",
        "symfony/routing": "^3.4|^4.2",
        "symplify/package-builder": "^6.0.3"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.9",
        "doctrine/orm": "^2.6",
        "symfony/framework-bundle": "^3.4|^4.2",
        "symfony/http-kernel": "^3.4|^4.2",
        "symfony/twig-bundle": "^3.4|^4.2",
        "symfony/templating": "^3.4|^4.2",
        "symfony/translation": "^3.4|^4.2",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\Autodiscovery\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\Autodiscovery\\Tests\\": "tests"
        },
        "classmap": [
            "tests/Source/KernelProjectDir"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
