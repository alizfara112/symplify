{
    "name": "symplify/statie",
    "description": "Static Site Generator",
    "license": "MIT",
    "bin": [
        "bin/statie",
        "bin/statie.php",
        "bin/statie_bootstrap.php"
    ],
    "require": {
        "php": "^7.1",
        "erusev/parsedown-extra": "^0.7",
        "cpliakas/git-wrapper": "^1.7",

        "latte/latte": "^2.4|^3.0",

        "symfony/dependency-injection": "^3.3",
        "symfony/http-kernel": "^3.3",
        "symfony/config": "^3.3",
        "nette/caching": "^2.4|^3.0",
        "nette/finder": "^2.4|^3.0",
        "nette/utils": "^2.4|^3.0",
        "tracy/tracy": "^2.4",

        "symfony/console": "^3.3",
        "symfony/finder": "^3.3",
        "symfony/translation": "^3.3",
        "symfony/yaml": "^3.3",

        "symplify/package-builder": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\Statie\\": "src",
            "Symplify\\Statie\\FlatWhite\\": "packages/FlatWhite/src",
            "Symplify\\Statie\\RelatedPosts\\": "packages/RelatedPosts/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\Statie\\Tests\\": "tests",
            "Symplify\\Statie\\FlatWhite\\Tests\\": "packages/FlatWhite/tests",
            "Symplify\\Statie\\RelatedPosts\\Tests\\": "packages/RelatedPosts/tests"
        }
    }
}
