{
    "name": "symplify/statie",
    "description": "Static Site Generator",
    "license": "MIT",
    "bin": [
        "bin/statie"
    ],
    "require": {
        "php": "^7.1",
        "erusev/parsedown-extra": "^0.7.1",
        "erusev/parsedown": "^1.7",
        "latte/latte": "^2.4",
        "symfony/config": "^3.4|^4.0",
        "symfony/dependency-injection": "^3.4|^4.0",
        "symfony/event-dispatcher": "^3.4|^4.0",
        "symfony/http-kernel": "^3.4|^4.0",
        "nette/finder": "^2.4",
        "nette/utils": "^2.5",
        "tracy/tracy": "^2.4",
        "twig/twig": "^2.5",
        "symfony/console": "^3.4|^4.0",
        "symfony/finder": "^3.4|^4.0",
        "symfony/yaml": "^3.4|^4.0",
        "symplify/package-builder": "^4.7",
        "jean85/pretty-package-versions": "^1.1",
        "ocramius/package-versions": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\Statie\\": "src",
            "Symplify\\Statie\\Latte\\": "packages/Latte/src",
            "Symplify\\Statie\\Generator\\": "packages/Generator/src",
            "Symplify\\Statie\\Twig\\": "packages/Twig/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\Statie\\Tests\\": "tests",
            "Symplify\\Statie\\Latte\\Tests\\": "packages/Latte/tests",
            "Symplify\\Statie\\Generator\\Tests\\": "packages/Generator/tests",
            "Symplify\\Statie\\Twig\\Tests\\": "packages/Twig/tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.7-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
