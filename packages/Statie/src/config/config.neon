application:
	scanDirs: FALSE
	scanComposer: FALSE

extensions:
    statie: Symplify\Statie\DI\StatieCompilerExtension
    - Symplify\Statie\FlatWhite\Adapter\Nette\DI\FlatWhiteExtension
    - Symplify\ModularLatteFilters\DI\ModularLatteFiltersExtension
    # Symfony\Console to Nette integration
    - Contributte\Console\DI\ConsoleExtension

statie:
    postRoute: blog/:year/:month/:day/:filename
