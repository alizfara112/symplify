{
    "name": "symplify/symfony-event-dispatcher",
    "description": "Symfony\\EventDispatcher integration to Nette.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "nette/di": "^2.4",
        "nette/http": "^2.4",
        "symfony/event-dispatcher": "^3.2",
        "symfony/dependency-injection": "^3.2",
        "symplify/package-builder": "@dev"
    },
    "require-dev": {
        "kdyby/console": "^2.4",
        "kdyby/events": "^3.2",
        "nette/bootstrap": "^2.4",
        "nette/application": "^2.4",
        "tracy/tracy": "^2.4",
        "symplify/coding-standard": "^1.3",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\SymfonyEventDispatcher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\SymfonyEventDispatcher\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/symplify-cs check src tests",
        "fix-cs": "vendor/bin/symplify-cs fix src tests"
    }
}
