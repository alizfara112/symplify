{
    "name": "symplify/symfony-event-dispatcher",
    "description": "Symfony\\EventDispatcher integration to Nette.",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "nette/di": "~2.4",
        "nette/http": "~2.4",
        "symfony/event-dispatcher": "~3.1"
    },
    "require-dev": {
        "kdyby/console": "~2.4",
        "kdyby/events": "~3.0",
        "nette/bootstrap": "~2.4",
        "nette/application": "~2.4",
        "tracy/tracy": "~2.4",
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\SymfonyEventDispatcher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\SymfonyEventDispatcher\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/symplify-cs check src tests",
        "fix-cs": "vendor/bin/symplify-cs fix src tests"
    }
}
