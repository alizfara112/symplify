{
    "name": "symplify/neon-to-yaml-converter",
    "description": "Converts Neon to Yaml",
    "license": "MIT",
    "bin": [
        "bin/neon-to-yaml-converter"
    ],
    "require": {
        "php": "^7.2",
        "nette/utils": "^3.0",
        "nette/neon": "^3.0",
        "symplify/package-builder": "^7.3",
        "symplify/smart-file-system": "^7.3",
        "symplify/autowire-array-parameter": "^7.3",
        "symfony/http-kernel": "^4.4|^5.0",
        "symfony/yaml": "^4.4|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\NeonToYamlConverter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\NeonToYamlConverter\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.3-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
