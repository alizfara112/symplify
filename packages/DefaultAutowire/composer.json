{
    "name": "symplify/default-autowire",
    "description": "Adds autowire to services when needed.",
    "require": {
        "php": "^7.1",
        "symfony/config": "^3.3",
        "symfony/dependency-injection": "^3.3",
        "symfony/http-kernel": "^3.3"
    },
    "require-dev": {
        "nette/utils": "^2.4",
        "nette/finder": "^2.4",
        "tracy/tracy": "^2.4",
        "phpunit/phpunit": "^6.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "symfony/form": "^3.3",
        "symfony/yaml": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\DefaultAutowire\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\DefaultAutowire\\Tests\\": "tests"
        }
    }
}
