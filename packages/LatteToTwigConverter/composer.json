{
    "name": "symplify/latte-to-twig-converter",
    "description": "Latte to Twig converter converts Latte to Twig",
    "license": "MIT",
    "bin": [
        "bin/latte-to-twig-converter"
    ],
    "require": {
        "php": "^7.2",
        "nette/utils": "^3.0",
        "symfony/dependency-injection": "^3.4.10|^4.3|^5.0",
        "symfony/console": "^3.4|^4.3|^5.0",
        "symfony/http-kernel": "^3.4|^4.3|^5.0",
        "symfony/finder": "^3.4|^4.3|^5.0",
        "symplify/package-builder": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.4"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\LatteToTwigConverter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\LatteToTwigConverter\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
