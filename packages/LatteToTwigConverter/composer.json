{
    "name": "symplify/latte-to-twig-converter",
    "description": "Latte to Twig converter converts Latte to Twig",
    "license": "MIT",
    "bin": [
        "bin/latte-to-twig-converter"
    ],
    "require": {
        "php": "^7.1",
        "nette/utils": "^2.5",
        "symfony/dependency-injection": "^3.4|^4.0",
        "symfony/console": "^3.4|^4.0",
        "symfony/http-kernel": "^3.4|^4.0",
        "symfony/finder": "^3.4|^4.0",
        "thecodingmachine/safe": "^0.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "symplify/package-builder": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\LatteToTwigConverter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\LatteToTwigConverter\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.8-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
