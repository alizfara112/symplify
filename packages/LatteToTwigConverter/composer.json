{
    "name": "symplify/latte-to-twig-converter",
    "description": "Latte to Twig converter converts Latte to Twig",
    "license": "MIT",
    "bin": [
        "bin/latte-to-twig-converter"
    ],
    "require": {
        "php": "^7.1",
        "nette/utils": "^2.5",
        "symfony/dependency-injection": "^3.4|^4.1",
        "symfony/console": "^3.4|^4.1",
        "symfony/http-kernel": "^3.4|^4.1",
        "symfony/finder": "^3.4|^4.1",
        "thecodingmachine/safe": "^0.1.9",
        "symplify/package-builder": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\LatteToTwigConverter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\LatteToTwigConverter\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
