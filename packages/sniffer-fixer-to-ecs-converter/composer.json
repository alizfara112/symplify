{
    "name": "symplify/sniffer-fixer-to-ecs-converter",
    "description": "Converter PHP_CodeSniffer or PHP-CS-Fixer configs to ECS with single Command",
    "license": "MIT",
    "bin": [
        "bin/sniffer-fixer-to-ecs-conveter"
    ],
    "require": {
        "php": ">=7.3",
        "ext-simplexml": "*",
        "nette/utils": "^3.0",
        "nette/robot-loader": "^3.2",
        "symfony/dependency-injection": "^5.1",
        "symfony/console": "^4.4|^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symplify/php-config-printer": "^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "friendsofphp/php-cs-fixer": "^2.16",
        "symplify/symplify-kernel": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.4"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\SnifferFixerToECSConverter\\": "src"
        },
        "files": [
            "stubs/Sniff.php",
            "vendor/squizlabs/php_codesniffer/autoload.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\SnifferFixerToECSConverter\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
