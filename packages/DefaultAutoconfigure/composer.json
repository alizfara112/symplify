{
    "name": "symplify/default-autoconfigure",
    "description": "Autoconfigures service tags when needed.",
    "require": {
        "php": "^7.1",
        "symfony/dependency-injection": "3.3.x-dev"
    },
    "require-dev": {
        "symfony/console": "3.3.x-dev",
        "symfony/http-kernel": "3.3.x-dev",
        "symfony/framework-bundle": "3.3.x-dev",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\DefaultAutoconfigure\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\DefaultAutoconfigure\\Tests\\": "tests"
        }
    }
}
