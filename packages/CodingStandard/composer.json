{
    "name": "symplify/coding-standard",
    "description": "Set of Symplify rules for PHP_CodeSniffer.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "nette/utils": "^2.4",
        "nette/finder": "^2.4",
        "squizlabs/php_codesniffer": "^3.2",
        "friendsofphp/php-cs-fixer": "^2.9",
        "symplify/token-runner": "^3.0"
    },
    "require-dev": {
        "nette/application": "^2.4",
        "symplify/easy-coding-standard": "^3.0",
        "symplify/package-builder": "^3.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\CodingStandard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\CodingStandard\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
