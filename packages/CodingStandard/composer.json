{
    "name": "symplify/coding-standard",
    "description": "Set of Symplify rules for PHP_CodeSniffer.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "nette/utils": "^2.4|^3.0",
        "nette/finder": "^2.4|^3.0",
        "slevomat/coding-standard": "^3.0",
        "squizlabs/php_codesniffer": "^3.0.1",
        "nikic/php-parser": "^3.0"
    },
    "require-dev": {
        "symplify/easy-coding-standard": "^2.0",
        "symplify/package-builder": "^2.0",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\CodingStandard\\": "src",
            "Symplify\\CodingStandard\\TokenWrapper\\": "packages/TokenWrapper/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\CodingStandard\\Tests\\": "tests"
        }
    }
}
