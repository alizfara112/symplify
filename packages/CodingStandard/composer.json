{
    "name": "symplify/coding-standard",
    "description": "Set of Symplify rules for PHP_CodeSniffer.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "nette/utils": "^2.4",
        "nette/finder": "^2.4",
        "squizlabs/php_codesniffer": "^3.2",
        "friendsofphp/php-cs-fixer": "^2.12",
        "symplify/token-runner": "^4.4"
    },
    "require-dev": {
        "nette/application": "^2.4",
        "symplify/easy-coding-standard-tester": "^4.4",
        "symplify/package-builder": "^4.4",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\CodingStandard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\CodingStandard\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
