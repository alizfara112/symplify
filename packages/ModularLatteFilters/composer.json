{
	"name": "symplify/modular-latte-filters",
	"description": "Nette extension for modular Latte filters loading.",
	"license": "MIT",
	"require": {
		"php": "^7.1",
		"nette/di": "^2.4",
		"latte/latte": "^2.4"
	},
	"require-dev": {
		"nette/application": "^2.4",
		"nette/bootstrap": "^2.4",
		"tracy/tracy": "^2.4",
		"phpunit/phpunit": "^6.0",
		"symplify/coding-standard": "^4.2"
	},
	"autoload": {
		"psr-4": {
			"Symplify\\ModularLatteFilters\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Symplify\\ModularLatteFilters\\Tests\\": "tests"
		}
	},
	"scripts": {
		"check-cs": "vendor/bin/phpcs src tests -ps --standard=vendor/symplify/coding-standard/src/SymplifyCodingStandard/ruleset.xml"
	}
}
