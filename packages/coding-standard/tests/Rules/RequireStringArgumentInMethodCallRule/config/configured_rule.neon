        arguments:
            stringArgByMethodByType:
                Symplify\CodingStandard\Tests\Rules\RequireStringArgumentInMethodCallRule\Source\AlwaysCallMeWithString:
                    callMe: [1]

    - Symplify\CodingStandard\PhpParser\NodeNameResolver
