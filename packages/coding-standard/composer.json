{
    "name": "symplify/coding-standard",
    "description": "Set of Symplify rules for PHP_CodeSniffer and PHP CS Fixer.",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "nette/utils": "^3.0",
        "nette/finder": "^2.5",
        "squizlabs/php_codesniffer": "^3.5",
        "slevomat/coding-standard": "^6.4",
        "friendsofphp/php-cs-fixer": "^2.16",
        "symplify/symplify-kernel": "^8.4",
        "symplify/package-builder": "^8.4",
        "symplify/autowire-array-parameter": "^8.4"
    },
    "require-dev": {
        "nette/application": "^3.0",
        "nette/bootstrap": "^3.0",
        "symplify/easy-coding-standard-tester": "^8.4",
        "symfony/http-kernel": "^4.4|^5.1",
        "doctrine/orm": "^2.7",
        "symplify/smart-file-system": "^8.4",
        "symfony/framework-bundle": "^4.4|^5.1",
        "migrify/php-config-printer": "^0.3.50",
        "phpunit/phpunit": "^8.5|^9.4"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\CodingStandard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\CodingStandard\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
