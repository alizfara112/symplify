{
    "name": "symplify/coding-standard",
    "description": "Set of Symplify rules for PHP_CodeSniffer and PHP CS Fixer.",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "nette/utils": "^3.2",
        "friendsofphp/php-cs-fixer": "^3.0",
        "symplify/symplify-kernel": "^9.5",
        "symplify/package-builder": "^9.5",
        "symplify/autowire-array-parameter": "^9.5",
        "symplify/rule-doc-generator-contracts": "^9.5"
    },
    "require-dev": {
        "nette/application": "^3.1",
        "nette/bootstrap": "^3.1",
        "symfony/http-kernel": "^5.3",
        "doctrine/orm": "^2.9",
        "symplify/smart-file-system": "^9.5",
        "symfony/framework-bundle": "^5.3",
        "symplify/easy-coding-standard": "^9.5",
        "phpunit/phpunit": "^9.5",
        "symplify/rule-doc-generator": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\CodingStandard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\CodingStandard\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
