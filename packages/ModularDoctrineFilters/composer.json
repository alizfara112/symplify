{
    "name": "symplify/modular-doctrine-filters",
    "description": "Adds simple service support for Doctrine filters, allowing modularity.",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "doctrine/dbal": "^2.4",
        "doctrine/orm": "^2.4",
        "symfony/config": "^3.2",
        "symfony/dependency-injection": "^3.2",
        "symfony/event-dispatcher": "^3.2",
        "symfony/http-kernel": "^3.2"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.6",
        "nette/utils": "^2.4",
        "nette/finder": "^2.4",
        "phpunit/phpunit": "^5.7",
        "symfony/form": "^3.2",
        "symfony/framework-bundle": "^3.2",
        "symfony/validator": "^3.2",
        "symplify/coding-standard": "^1.2",
        "twig/twig": "^1.30"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ModularDoctrineFilters\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ModularDoctrineFilters\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/symplify-cs check src tests",
        "fix-cs": "vendor/bin/symplify-cs fix src tests"
    }
}
