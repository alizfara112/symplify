application:
	scanComposer: false


services:
	- Symplify\DoctrineFilters\Tests\FilterManager\Source\ActiveFilter
	- Symplify\DoctrineFilters\Tests\FilterManager\Source\EnabledFilter
	- Symplify\DoctrineFilters\Tests\FilterManager\Source\DisabledFilter
	-
		class: Symfony\Component\Console\Application
		setup:
			- setDispatcher


extensions:
	doctrine: Kdyby\Doctrine\DI\OrmExtension
	- Kdyby\Events\DI\EventsExtension
	- Kdyby\Annotations\DI\AnnotationsExtension
	- Symplify\DoctrineFilters\DI\ModularDoctrineFiltersExtension
	- Symplify\SymfonyEventDispatcher\Adapter\Nette\DI\SymfonyEventDispatcherExtension


doctrine:
	driver: pdo_sqlite
	memory: true
