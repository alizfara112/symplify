{
    "name": "symplify/easy-coding-standard-tester",
    "description": "# The Best Way to Test Sniffs and Fixers",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "phpunit/phpunit": "^7.5|^8.4",
        "symplify/easy-coding-standard": "^6.2",
        "symplify/package-builder": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\EasyCodingStandardTester\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\EasyCodingStandardTester\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
