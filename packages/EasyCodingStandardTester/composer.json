{
    "name": "symplify/easy-coding-standard-tester",
    "description": "# The Best Way to Test Sniffs and Fixers",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "phpunit/phpunit": "^7.5",
        "symplify/easy-coding-standard": "^5.2.18",
        "symplify/package-builder": "^5.2.18",
        "thecodingmachine/safe": "^0.1.8"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\EasyCodingStandardTester\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\EasyCodingStandardTester\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.3-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
