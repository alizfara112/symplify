{
    "name": "symplify/easy-coding-standard-tester",
    "description": "# The Best Way to Test Sniffs and Fixers",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "phpunit/phpunit": "^7.3",
        "symplify/easy-coding-standard": "^5.0",
        "symplify/package-builder": "^5.0",
        "thecodingmachine/safe": "^0.1.3"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\EasyCodingStandardTester\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\EasyCodingStandardTester\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
