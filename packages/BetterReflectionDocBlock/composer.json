{
    "name": "symplify/better-reflection-docblock",
    "description": "Slim wrapper around phpdocumentor/reflection-docblock with better DX and simpler API.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "phpdocumentor/reflection-docblock": "5.0.0-alpha1",
        "phpstan/phpdoc-parser": "^0.2.0",
        "symplify/package-builder": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\BetterReflectionDocBlock\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\BetterReflectionDocBlock\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
