{
    "name": "symplify/better-reflection-docblock",
    "description": "Slim wrapper around phpdocumentor/reflection-docblock with better DX and simpler API.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "phpdocumentor/reflection-docblock": "4.2|4.3",
        "symplify/package-builder": "^4.0",
        "symplify/token-runner": "^4.0",
        "friendsofphp/php-cs-fixer": "~2.10.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\BetterReflectionDocBlock\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\BetterReflectionDocBlock\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
