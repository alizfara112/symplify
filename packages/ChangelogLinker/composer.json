{
    "name": "symplify/changelog-linker",
    "description": "Generates beautiful CHANGELOG.md with links to PRs, versions and users grouped in Added/Changed/Fixed/Removed categories.",
    "license": "MIT",
    "bin": [
        "bin/changelog-linker"
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.4",
        "nette/utils": "^3.0",
        "symfony/console": "^3.4|^4.3|^5.0",
        "symfony/http-kernel": "^3.4|^4.3|^5.0",
        "symfony/process": "^3.4|^4.3|^5.0",
        "symplify/package-builder": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.4"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ChangelogLinker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ChangelogLinker\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
