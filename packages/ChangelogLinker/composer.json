{
    "name": "symplify/changelog-linker",
    "description": "Add links to issues, PRs, versions, commits and users in your CHANGELOG.md",
    "license": "MIT",
    "bin": [
        "bin/changelog-linker",
        "bin/changelog-linker-bootstrap.php"
    ],
    "require": {
        "php": "^7.1",
        "nette/utils": "^2.4",
        "symfony/console": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ChangelogLinker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ChangelogLinker\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
