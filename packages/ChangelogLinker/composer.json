{
    "name": "symplify/changelog-linker",
    "description": "Add links to issues, PRs, versions, commits and users in your CHANGELOG.md",
    "license": "MIT",
    "bin": [
        "bin/changelog-linker",
        "bin/changelog-linker-bootstrap.php",
        "bin/changelog-linker-container.php"
    ],
    "require": {
        "php": "^7.1",
        "nette/utils": "^2.5",
        "symfony/console": "^3.4|^4.0",
        "symplify/package-builder": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ChangelogLinker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ChangelogLinker\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
