{
    "name": "symplify/changelog-linker",
    "description": "Generates beautiful CHANGELOG.md with links to PRs, versions and users grouped in Added/Changed/Fixed/Removed categories.",
    "license": "MIT",
    "bin": [
        "bin/changelog-linker"
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.3",
        "nette/utils": "^2.5",
        "symfony/console": "^3.4|^4.1",
        "symfony/http-kernel": "^3.4|^4.1",
        "symfony/process": "^3.4|^4.1",
        "symplify/package-builder": "^4.9",
        "thecodingmachine/safe": "^0.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ChangelogLinker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ChangelogLinker\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.9-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
