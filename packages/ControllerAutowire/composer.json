{
    "name": "symplify/controller-autowire",
    "description": "Adds support to autowire controller constructor without pain.",
    "require": {
        "php": "^7.1",
        "nette/robot-loader": "^2.4",
        "symfony/config": "^3.2",
        "symfony/dependency-injection": "^3.2",
        "symfony/http-kernel": "^3.2",
        "symfony/yaml": "^3.2",
        "symfony/serializer": "^3.2",
        "symfony/security": "^3.2"
    },
    "require-dev": {
        "nette/utils": "^2.4",
        "nette/finder": "^2.4",
        "symfony/framework-bundle": "^3.2",
        "phpunit/phpunit": "^6.0",
        "doctrine/annotations": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ControllerAutowire\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ControllerAutowire\\Tests\\": "tests"
        }
    }
}
