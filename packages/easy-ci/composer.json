{
    "name": "symplify/easy-ci",
    "description": "Toolkit of commands that should not be missed in you CI",
    "license": "MIT",
    "bin": [
        "bin/easy-ci"
    ],
    "require": {
        "php": ">=8.0",
        "composer/semver": "^3.2",
        "symfony/dependency-injection": "^5.3.x-dev",
        "symfony/console": "^5.3.x-dev",
        "symfony/http-kernel": "^5.3",
        "nette/utils": "^3.2",
        "nikic/php-parser": "^4.11",
        "symplify/composer-json-manipulator": "^9.4.22",
        "symplify/package-builder": "^9.4.22",
        "symplify/symplify-kernel": "^9.4.22",
        "symplify/astral": "^9.4.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "latte/latte": "^2.10",
        "nette/application": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\EasyCI\\": [
                "src",
                "packages"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\EasyCI\\Tests\\": [
                "tests",
                "packages-tests"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
