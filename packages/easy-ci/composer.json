{
    "name": "symplify/easy-ci",
    "description": "Toolkit of commands that should not be missed in you CI",
    "license": "MIT",
    "bin": [
        "bin/easy-ci"
    ],
    "require": {
        "php": ">=7.3",
        "composer/semver": "^3.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/console": "^4.4|^5.2",
        "symfony/http-kernel": "^4.4|^5.2",
        "nette/utils": "^3.2",
        "nikic/php-parser": "4.10.4",
        "symplify/composer-json-manipulator": "^9.3.4",
        "symplify/package-builder": "^9.3.4",
        "symplify/symplify-kernel": "^9.3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "latte/latte": "^2.10",
        "nette/application": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\EasyCI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\EasyCI\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
