{
    "name": "symplify/easy-coding-standard-tester",
    "description": "The Best Way to Test Sniffs and Fixers",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "phpunit/phpunit": "^9.5",
        "symplify/easy-coding-standard": "^9.3",
        "symplify/package-builder": "^9.3",
        "symplify/easy-testing": "^9.3",
        "symplify/smart-file-system": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\EasyCodingStandardTester\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\EasyCodingStandardTester\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
