{
    "name": "symplify/easy-coding-standard-tester",
    "description": "The Best Way to Test Sniffs and Fixers",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "phpunit/phpunit": "^8.5|^9.0",
        "symplify/easy-coding-standard": "^8.2.15",
        "symplify/package-builder": "^8.2.15",
        "symplify/easy-testing": "^8.2.15",
        "symplify/smart-file-system": "^8.2.15"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\EasyCodingStandardTester\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\EasyCodingStandardTester\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.3-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
