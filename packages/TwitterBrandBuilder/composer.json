{
    "name": "symplify/twitter-brand-builder",
    "description": "Twitter methods for easier brand building: buffer, value content, AI and bot simulation.",
    "license": "MIT",
    "authors": [
        { "name": "Tomas Votruba", "email": "tomas.vot@gmail.com", "homepage": "http://tomasvotruba.cz" }
    ],
    "require": {
        "php": "^7.0",
        "abraham/twitteroauth": "~0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "symplify/coding-standard": "^1.0",
        "tracy/tracy": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\TwitterBrandBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\TwitterBrandBuilder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "fix-cs": "vendor/bin/symplify-cs fix src tests",
        "check-cs": "vendor/bin/symplify-cs check src tests"
    }
}
