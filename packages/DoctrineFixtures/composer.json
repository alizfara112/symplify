{
	"name": "symplify/doctrine-fixtures",
	"description": "Doctrine Fixtures, Faker and Alice integration into Nette",
	"license": "MIT",
	"require": {
		"php": "^7.1",
		"fzaninotto/faker": "^1.6",
		"nelmio/alice": "^2.2",
		"kdyby/doctrine": "^3.1",

		"nette/di": "^2.4",
		"nette/finder": "^2.4",
		"nette/reflection": "^2.4",
		"symfony/console": "^3.2",
		"tracy/tracy": "^2.4"
	},
	"require-dev": {
		"symplify/coding-standard": "^4.2",
		"nette/bootstrap": "^2.4",
		"phpunit/phpunit": "^6.0"
	},
	"autoload": {
		"psr-4": {
			"Symplify\\DoctrineFixtures\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Symplify\\DoctrineFixtures\\Tests\\": "tests"
		}
	},
	"scripts": {
		"check-cs": "vendor/bin/phpcs src tests -ps --standard=vendor/symplify/coding-standard/src/SymplifyCodingStandard/ruleset.xml"
	}
}
