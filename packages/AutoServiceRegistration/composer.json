{
    "name": "symplify/auto-service-registration",
    "description": "Register your service by suffix scan instead of manual writing.",
    "license": "MIT",
    "require": {
        "php": "~7.0",
        "nette/robot-loader": "^2.4",
        "symfony/config": "^3.1",
        "symfony/dependency-injection": "^3.1",
        "symfony/http-kernel": "^3.1"
    },
    "require-dev": {
        "nette/utils": "^2.4",
        "symplify/coding-standard": "^2.0",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\AutoServiceRegistration\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\AutoServiceRegistration\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/symplify-cs check src tests",
        "fix-cs": "vendor/bin/symplify-cs fix src tests"
    }
}
