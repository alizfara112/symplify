{
    "name": "symplify/auto-service-registration",
    "description": "Register your service by suffix scan instead of manual writing.",
    "require": {
        "php": "^7.1",
        "nette/robot-loader": "^3.0",
        "symfony/config": "^3.2",
        "symfony/dependency-injection": "^3.2",
        "symfony/http-kernel": "^3.2",
        "symfony/yaml": "^3.2"
    },
    "require-dev": {
        "nette/utils": "^2.4",
        "nette/finder": "^2.4",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\AutoServiceRegistration\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\AutoServiceRegistration\\Tests\\": "tests"
        }
    }
}
