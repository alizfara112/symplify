{
    "name": "symplify/token-runner",
    "description": "Set of utils for PHP_CodeSniffer and PHP CS Fixer.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "nette/utils": "^2.5",
        "nette/finder": "^2.4",
        "symplify/package-builder": "^5.3.3",
        "symplify/better-phpdoc-parser": "^5.3.3",
        "friendsofphp/php-cs-fixer": "^2.14",
        "squizlabs/php_codesniffer": "^3.4",
        "thecodingmachine/safe": "^0.1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\TokenRunner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\TokenRunner\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
