{
    "name": "symplify/token-runner",
    "description": "Set of utils for PHP_CodeSniffer and PHP CS Fixer.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "nette/utils": "^2.4",
        "nette/finder": "^2.4",
        "symplify/package-builder": "^3.2",
        "symplify/better-reflection-docblock": "^3.2",
        "friendsofphp/php-cs-fixer": "2.10",
        "squizlabs/php_codesniffer": "^3.2",
        "phpdocumentor/reflection-docblock": "4.2|4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\TokenRunner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\TokenRunner\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.3-dev"
        }
    },
    "minimum-stability": "dev"
}
