{
    "name": "symplify/symfony-security-voters",
    "description": "Voters features from Symfony\\Security for standalone use.",
    "require": {
        "php": "^7.1",
        "nette/di": "^2.4",
        "nette/http": "^2.4",
        "symfony/security-core": "^3.2",
        "symplify/symfony-event-dispatcher": "^1.2"
    },
    "require-dev": {
        "nette/application": "^2.4",
        "nette/bootstrap": "^2.4",
        "nette/utils": "^2.4",
        "phpunit/phpunit": "^6.0",
        "tracy/tracy": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\SymfonySecurityVoters\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\SymfonySecurityVoters\\Tests\\": "tests"
        }
    }
}
