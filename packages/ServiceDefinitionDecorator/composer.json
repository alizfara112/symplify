{
    "name": "symplify/service-definition-decorator",
    "description": "Decorate service definitions by type, with autowire, tags and method class.",
    "require": {
        "php": "^7.1",
        "symfony/dependency-injection": "^3.3",
        "symfony/http-kernel": "^3.3"
    },
    "require-dev": {
        "symfony/console": "^3.3",
        "symfony/event-dispatcher": "^3.3",
        "symfony/framework-bundle": "^3.3",
        "symfony/yaml": "^3.3",
        "doctrine/annotations": "^1.3",
        "nette/utils": "^2.4",
        "nette/finder": "^2.4",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ServiceDefinitionDecorator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ServiceDefinitionDecorator\\Tests\\": "tests"
        }
    }
}
