            "Symplify\\MonorepoBuilder\\Release\\Tests\\": "packages/monorepo-builder/packages/release/tests",
            "Symplify\\MonorepoBuilder\\Testing\\Tests\\": "packages/monorepo-builder/packages/testing/tests",
            "Symplify\\MonorepoBuilder\\Tests\\": "packages/monorepo-builder/tests",
            "Symplify\\NeonToYamlConverter\\Tests\\": "packages/neon-to-yaml-converter/tests",
