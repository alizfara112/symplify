        "symplify/autodiscovery": "self.version",
        "symplify/autowire-array-parameter": "self.version",
        "symplify/changelog-linker": "self.version",
        "symplify/coding-standard": "self.version",
        "symplify/composer-json-manipulator": "self.version",
        "symplify/config-transformer": "self.version",
        "symplify/console-color-diff": "self.version",
        "symplify/easy-ci": "self.version",
        "symplify/easy-coding-standard": "self.version",
        "symplify/easy-coding-standard-tester": "self.version",
        "symplify/easy-hydrator": "self.version",
        "symplify/easy-testing": "self.version",
        "symplify/flex-loader": "self.version",
        "symplify/latte-to-twig-converter": "self.version",
        "symplify/markdown-diff": "self.version",
        "symplify/monorepo-builder": "self.version",
        "symplify/neon-to-yaml-converter": "self.version",
        "symplify/package-builder": "self.version",
        "symplify/package-scoper": "self.version",
        "symplify/php-config-printer": "self.version",
        "symplify/phpstan-extensions": "self.version",
        "symplify/phpstan-php-config": "self.version",
        "symplify/phpstan-rules": "self.version",
        "symplify/phpunit-upgrader": "self.version",
        "symplify/psr4-switcher": "self.version",
        "symplify/rule-doc-generator": "self.version",
        "symplify/set-config-resolver": "self.version",
        "symplify/skipper": "self.version",
        "symplify/smart-file-system": "self.version",
        "symplify/static-detector": "self.version",
        "symplify/symfony-static-dumper": "self.version",
        "symplify/symplify-kernel": "self.version",
        "symplify/template-checker": "self.version",
        "symplify/vendor-patches": "self.version"
    },
    "scripts": {
        "complete-check": [
            "phpunit",
            "@fix-cs",
            "@phpstan"
        ],
        "check-cs": "packages/easy-coding-standard/bin/ecs check --ansi",
        "fix-cs": [
            "packages/easy-coding-standard/bin/ecs check --fix --ansi",
            "packages/easy-coding-standard/bin/ecs check-markdown README.md packages/**/README.md --fix --ansi",
            "bin/clear_readmes.sh"
        ],
        "phpstan": "vendor/bin/phpstan analyse --error-format symplify --ansi",
        "docs": [
            "packages/rule-doc-generator/bin/rule-doc-generator generate packages/coding-standard --output-file packages/coding-standard/docs/rules_overview.md",
            "packages/easy-coding-standard/bin/ecs check-markdown packages/coding-standard/docs/rules_overview.md --fix",
            "packages/rule-doc-generator/bin/rule-doc-generator generate packages/phpstan-rules --output-file packages/phpstan-rules/docs/rules_overview.md",
            "packages/easy-coding-standard/bin/ecs check-markdown packages/phpstan-rules/docs/rules_overview.md --fix"
        ],
        "changelog": [
            "packages/changelog-linker/bin/changelog-linker dump-merges --in-categories --in-packages --ansi",
            "packages/changelog-linker/bin/changelog-linker link --ansi",
            "packages/changelog-linker/bin/changelog-linker cleanup --ansi"
        ],
        "merge": "packages/monorepo-builder/bin/monorepo-builder merge --ansi",
        "propagate": "packages/monorepo-builder/bin/monorepo-builder propagate --ansi",
        "rector": "vendor/bin/rector process --config rector-ci.php --dry-run --ansi",
        "validate-monorepo": "packages/monorepo-builder/bin/monorepo-builder validate --ansi",
        "release": "packages/monorepo-builder/bin/monorepo-builder release patch --ansi"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
